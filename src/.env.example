# App Settings
APP_NAME="FastAPI app"
APP_DESCRIPTION="Your app description"
APP_VERSION="1.0.0"
LICENSE="MIT"
CONTACT_NAME="Your Name"
CONTACT_EMAIL="your.email@example.com"

# Environment Settings
ENVIRONMENT="local"
DB_ENGINE="postgres"

# Security Settings
SECRET_KEY="your-secret-key-here"
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7
CLERK_SECRET_KEY="your-clerk-secret-key"
CLERK_SIGNING_SECRET="your-clerk-signing-secret"

# External Database Settings
POSTGRES_USER="your_db_user"
POSTGRES_PASSWORD="your_db_password"
POSTGRES_SERVER="your.database.host"
POSTGRES_PORT=5432
POSTGRES_DB="your_database_name"


# Redis Settings
REDIS_CACHE_HOST="localhost"
REDIS_CACHE_PORT=6379
REDIS_QUEUE_HOST="localhost"
REDIS_QUEUE_PORT=6379
REDIS_RATE_LIMIT_HOST="localhost"
REDIS_RATE_LIMIT_PORT=6379

# Rate Limiting
DEFAULT_RATE_LIMIT_LIMIT=10
DEFAULT_RATE_LIMIT_PERIOD=3600

# Client Cache
CLIENT_CACHE_MAX_AGE=60

# Admin User Settings
ADMIN_NAME="admin"
ADMIN_EMAIL="admin@example.com"
ADMIN_USERNAME="admin"
ADMIN_PASSWORD="change-this-password"

# Test Settings
TEST_NAME="Tester User"
TEST_EMAIL="test@example.com"
TEST_USERNAME="tester"
TEST_PASSWORD="test-password"